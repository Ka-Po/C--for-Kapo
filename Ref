using System;

namespace Ref
{
    class MainClass
    {
        public static void Main(string[] args)
        {
            int a, b;
            a = 1;
            b = 2;

            Swap(ref a,ref b);            //  位址引數

            Console.WriteLine($"{a},{b}");
        }

        static void Swap(ref int a,ref int b)   // 位址參數
        {
            int temp = a;
            a = b;
            b = temp;
        }
    }
}
// Console 顯示：
// 2,1
